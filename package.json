{
    "name": "react-outliner",
    "version": "0.0.9",
    "description": "A React UI component for typing notes.",
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "scripts": {
        "start": "node ./build/index.js",
        "start:dev": "nodemon",
        "c": "npm run clean",
        "clean": "rm -rf build/*",
        "b": "npm run build",
        "build": "tsc",
        "cb": "npm run clean-build",
        "clean-build": "npm run clean && npm run build",
        "bw": "npm run build:watch",
        "build:watch": "tsc --watch",
        "cbw": "npm run clean-build:watch",
        "clean-build:watch": "npm run clean-build && npm run build:watch",
        "prepublishOnly": "npm run clean && npm run lint && npm run build",
        "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
        "pretest": "npm run lint",
        "t": "npm run test",
        "test": "jest",
        "tw": "npm run test:watch",
        "test:watch": "jest --watch"
    },
    "repository": {
        "type": "git",
        "url": "git+https://ashleydavis@github.com/ashleydavis/react-outliner.git"
    },
    "keywords": [],
    "author": "ashley@codecapers.com.au",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ashleydavis/react-outliner/issues"
    },
    "homepage": "https://github.com/ashleydavis/react-outliner#readme",
    "dependencies": {
        "react": "^17.0.1",
        "react-beautiful-dnd": "^13.0.0",
        "react-contenteditable": "^3.3.5",
        "react-dom": "^17.0.1"
    },
    "devDependencies": {
        "@types/enzyme": "^3.10.8",
        "@types/enzyme-adapter-react-16": "^1.0.6",
        "@types/jest": "^26.0.15",
        "@types/node": "^14.14.9",
        "@types/node-uuid": "0.0.28",
        "@types/react": "^17.0.0",
        "@types/react-beautiful-dnd": "^13.0.0",
        "@types/react-dom": "^17.0.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.5",
        "jest": "^26.6.3",
        "nodemon": "^2.0.6",
        "source-map-support": "0.5.19",
        "ts-jest": "^26.4.4",
        "ts-node": "^9.0.0",
        "tslint": "^6.1.3",
        "typescript": "^4.1.2"
    }
}
